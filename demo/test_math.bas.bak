Sub _Ready
    Print "Testing Math..."
    
    Dim Pi
    Pi = 3.1415926535

    Print "Sin(Pi/2): " & Sin(Pi/2)
    Print "Cos(Pi): " & Cos(Pi)
    Print "Sqr(16): " & Sqr(16)
    Print "Abs(-42): " & Abs(-42)
    Print "Int(42.9): " & Int(42.9)
    Print "Rnd: " & Rnd()
    
    Randomize
    Print "Rnd (Randomized): " & Rnd()
    
    GetTree().Quit()
End Sub
