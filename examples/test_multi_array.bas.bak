Sub Main
    Print "Testing Multidimensional Arrays"
    
    Dim Grid(2, 2) ' 3x3 Grid
    
    Grid(0, 0) = 1
    Grid(0, 1) = 2
    Grid(0, 2) = 3
    Grid(1, 0) = 4
    Grid(1, 1) = 5
    Grid(1, 2) = 6
    Grid(2, 0) = 7
    Grid(2, 1) = 8
    Grid(2, 2) = 9
    
    Print "Grid(1, 1) should be 5: "
    Print Grid(1, 1)
    
    Print "Grid(2, 0) should be 7: "
    Print Grid(2, 0)
    
    Dim Cube(1, 1, 1) ' 2x2x2 Cube
    Cube(0, 0, 0) = 100
    Cube(1, 1, 1) = 999
    
    Print "Cube(0,0,0): " 
    Print Cube(0,0,0)
    Print "Cube(1,1,1): " 
    Print Cube(1,1,1)
    
    Print "Done."
End Sub
