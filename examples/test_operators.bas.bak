Sub Main()
    Dim i
    i = 10
    Print "Initial i: " + i
    
    i++
    Print "After i++: " + i
    
    i+=5
    Print "After i+=5: " + i
    
    i--
    Print "After i--: " + i
    
    i-=2
    Print "After i-=2: " + i
    
    i*=2
    Print "After i*=2: " + i
    
    i/=4
    Print "After i/=4: " + i
    
End Sub
