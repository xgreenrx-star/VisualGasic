Sub Main()
    Print "Testing TwinBasic/Extended Commands..."
    
    ' Split and Join
    Dim s
    s = "Apple,Banana,Cherry"
    Print "Original: " + s
    
    Dim parts
    parts = Split(s, ",")
    
    Print "UBound: "
    Print UBound(parts)
    Print "Part 0: " + parts(0)
    Print "Part 1: " + parts(1)
    
    Dim joined
    joined = Join(parts, " - ")
    Print "Joined: " + joined
    
    ' Asc and Chr
    Print "Asc('A'): "
    Print Asc("A")
    Print "Chr(66): " + Chr(66)
    
    ' Space
    Print "Space(5) => '" + Space(5) + "'"

End Sub
